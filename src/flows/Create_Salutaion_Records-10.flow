    <decisions>
        <name>Check_If_Need_to_truncate_Account_Name</name>
        <label>Check If Need to truncate Account Name?</label>
        <locationX>65</locationX>
        <locationY>101</locationY>
        <defaultConnector>
            <targetReference>Create_Organization_Pref_Mail_Name_Salutation_without_truncation</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>[Default Outcome]</defaultConnectorLabel>
        <rules>
            <name>Truncate_Account_Name</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>AccountNameLen</leftValueReference>
                <operator>GreaterThan</operator>
                <rightValue>
                    <numberValue>20.0</numberValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Create_Organization_Pref_Mail_Name_Salutation</targetReference>
            </connector>
            <label>Truncate Account Name?</label>
        </rules>
    </decisions>
    <formulas>
        <name>AccountNameLen</name>
        <dataType>Number</dataType>
        <expression>LEN({!AccountName})</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>AccountNameWithoutSpace</name>
        <dataType>String</dataType>
        <expression>TRIM({!AccountName})</expression>
    </formulas>
    <formulas>
        <name>TruncatedAccountName</name>
        <dataType>String</dataType>
        <expression>LEFT({!AccountNameWithoutSpace}, 10)</expression>
    </formulas>
