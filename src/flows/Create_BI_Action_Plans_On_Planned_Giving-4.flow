    <description>This flow will create an action plan along with child tasks, according to action plan template from which action plan created.</description>
    <formulas>
        <name>actionPlanName</name>
        <dataType>String</dataType>
        <expression>{!plannedGivingName}+ &apos;-&apos;  + {!actionPlanApiName}</expression>
    </formulas>
    <label>Create BI Action Plans On Planned Giving</label>
    <loops>
        <name>Looping_All_Child_Task_to_Create_New_Taks_For_Action_Plan</name>
