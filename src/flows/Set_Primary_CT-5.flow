            <targetReference>Loop_through_all_CT_to_define_Primary</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>At least one CT</defaultConnectorLabel>
        <rules>
            <name>No_CTs</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>ContactConstituentTypes</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Set_Active_CT_to_Primary_CT</targetReference>
            </connector>
            <label>No CTs</label>
        </rules>
    </decisions>
    <description>This version has better comparison</description>
