    <decisions>
        <name>Compare_Priority</name>
        <label>Compare Priority</label>
        <locationX>550</locationX>
        <locationY>491</locationY>
        <defaultConnector>
            <targetReference>Loop_through_all_CT_to_define_Primary</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>[Default Outcome]</defaultConnectorLabel>
        <rules>
            <name>Current_CT_Lower</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>CurrentCT.rC_Bios__Priority__c</leftValueReference>
                <operator>LessThan</operator>
                <rightValue>
                    <elementReference>PrimaryCT.rC_Bios__Priority__c</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Set_CurrentCT_to_PrimaryCT</targetReference>
            </connector>
            <label>Current CT Lower</label>
        </rules>
    </decisions>
    <decisions>
        <name>One_or_More_CTs</name>
