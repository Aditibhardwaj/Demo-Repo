var result = sforce.apex.execute(&quot;AddRelatedContacts&quot;,&quot;populateContactField&quot;,{recordId : recordId,objectName : &apos;Contact_Report__c&apos;,accountId: accountId,firstContactId: firstContactId,secondContactId : secondContactId,firstContactAPIName : &apos;Contact__c&apos;,secondContactAPIName : &apos;Contact2__c&apos;});

if(result != &apos;&apos;){
    alert(result);
}else{
window.parent.location.href = &quot;/&quot;+recordId;
}</url>
