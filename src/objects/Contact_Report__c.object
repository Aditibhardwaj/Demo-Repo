        <protected>false</protected>
        <url>{! REQUIRESCRIPT(&quot;/soap/ajax/24.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/24.0/apex.js&quot;)} 

var firstContactId=&apos;{!Contact_Report__c.ContactId__c}&apos;; 
var accountId=&apos;{!Contact_Report__c.AccountId__c}&apos;; 
var recordId= &apos;{!Contact_Report__c.Id}&apos;;
